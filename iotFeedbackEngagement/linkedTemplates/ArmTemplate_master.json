{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "iotFeedbackEngagement"
		},
		"GBB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'GBB'",
			"defaultValue": "Integrated Security=True;Data Source=azoedbisqlprod;Initial Catalog=MSX_Engagement_Infrastructure"
		},
		"azoedbisqlprod_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azoedbisqlprod'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=iotbireportingsql.database.windows.net;Initial Catalog=InfraEngagement"
		},
		"linkedService1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedService1'"
		},
		"MSX_Infra_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MSX_Infra'",
			"defaultValue": "Integrated Security=True;Data Source=azoedbisqlprod;Initial Catalog=MSX_Engagement_Infrastructure"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://iotbikeyvault.vault.azure.net/"
		},
		"GBB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "redmond\\v-jouyang"
		},
		"linkedService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://iotbistorage.dfs.core.windows.net/"
		},
		"msxdatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cseoedlprodadlsg2wus201.dfs.core.windows.net/"
		},
		"msxdatalake_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"msxdatalake_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "3c4bff2e-cd35-4ba1-9a95-476924a4693e"
		},
		"integrationRuntime1_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/a311be7b-10b0-4f08-8f71-e8d304e87a18/resourcegroups/IOTBIReportting_Prod/providers/Microsoft.DataFactory/factories/IOTBI-SQL-Agent/integrationruntimes/IRFromVM2"
		},
		"MSX_Infra_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "redmond\\v-jouyang"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "iotFeedbackEngagement_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azoedbisqlprod_connectionString": {
						"value": "[parameters('azoedbisqlprod_connectionString')]"
					},
					"linkedService1_accountKey": {
						"value": "[parameters('linkedService1_accountKey')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"linkedService1_properties_typeProperties_url": {
						"value": "[parameters('linkedService1_properties_typeProperties_url')]"
					},
					"integrationRuntime1_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('integrationRuntime1_properties_typeProperties_linkedInfo_resourceId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "iotFeedbackEngagement_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
