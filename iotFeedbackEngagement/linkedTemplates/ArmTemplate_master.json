					},
					"GBB_connectionString": {
						"value": "[parameters('GBB_connectionString')]"
					},
					"MSX_Infra_connectionString": {
						"value": "[parameters('MSX_Infra_connectionString')]"
					},
					"GBB_properties_typeProperties_userName": {
						"value": "[parameters('GBB_properties_typeProperties_userName')]"
					},
					"msxdatalake_properties_typeProperties_url": {
						"value": "[parameters('msxdatalake_properties_typeProperties_url')]"
					},
					"msxdatalake_properties_typeProperties_tenant": {
						"value": "[parameters('msxdatalake_properties_typeProperties_tenant')]"
					},
					"msxdatalake_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('msxdatalake_properties_typeProperties_servicePrincipalId')]"
					},
					"MSX_Infra_properties_typeProperties_userName": {
						"value": "[parameters('MSX_Infra_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'iotFeedbackEngagement_ArmTemplate_0')]"
			]
		},
		{
			"name": "iotFeedbackEngagement_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'iotFeedbackEngagement_ArmTemplate_1')]"
			]
		}
	]
}